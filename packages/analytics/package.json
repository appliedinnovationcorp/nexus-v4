{
  "name": "@nexus/analytics",
  "version": "1.0.0",
  "description": "Product analytics integration with PostHog and Mixpanel for tracking user behaviors and feature usage",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "configs",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./server": {
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate-events": "node scripts/validate-events.js",
    "generate-types": "node scripts/generate-types.js",
    "analyze-usage": "node scripts/analyze-usage.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "analytics",
    "posthog",
    "mixpanel",
    "product-analytics",
    "user-tracking",
    "feature-usage",
    "behavioral-analytics"
  ],
  "author": "Nexus Team",
  "license": "MIT",
  "dependencies": {
    "posthog-js": "^1.96.1",
    "posthog-node": "^3.6.3",
    "mixpanel-browser": "^2.47.0",
    "mixpanel": "^0.17.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/mixpanel-browser": "^2.47.5",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.38",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-team/nexus-workspace.git",
    "directory": "packages/analytics"
  },
  "bugs": {
    "url": "https://github.com/nexus-team/nexus-workspace/issues"
  },
  "homepage": "https://nexus-analytics.dev"
}
